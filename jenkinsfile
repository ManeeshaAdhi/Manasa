pipeline{
     agent any
       tools{
              jdk "java-1.8"
              maven "maven-3.6.3"
       }
      stages{
             stage("clone"){
                steps{
                       git url: 'https://github.com/ManeeshaAdhi/Manasa.git'
                }
             }
             stage("build"){
                 steps{
                          sh 'mvn compile'
                 }
             }
      }	     
}
